<div class="row">
	<div class="col-xs-12">
	<h1>Log In</h1>


	<%= form_tag sessions_path do %>
	

	<div class="form-group">
		<%= label_tag :email %><br />
		<%= text_field_tag :email, params[:email] %>
	</div>
	<div class="form-group">
		<%= label_tag :password %><br />
		<%= password_field_tag :password %>
	</div>
	<div class="form-group">

		<div class="btn btn-default"><%= submit_tag "Log In" %></div>
	</div>
	<% end %>
</div>
</div>
